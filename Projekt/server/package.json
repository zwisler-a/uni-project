{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon",
    "test-data": "node -r ts-node/register ./src/test-data.ts config.json",
    "build": "tsc --removeComments",
    "test": "mocha -r ts-node/register test/**/*.ts --reporter mocha-junit-reporter --reporter-options mochaFile=test-report.xml",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "doc": "typedoc --out ./documentation/ ./src/"
  },
  "dependencies": {
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "debug": "~2.6.9",
    "express": "^4.16.4",
    "forever": "^0.15.3",
    "fs-extra": "^7.0.1",
    "jsonwebtoken": "^9.0.0",
    "mariadb": "^2.0.2-rc",
    "morgan": "~1.9.0",
    "multer": "^1.4.1",
    "node-cache": "^4.2.0",
    "nodemailer": "^5.1.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "^4.1.7",
    "@types/compression": "0.0.36",
    "@types/debug": "0.0.31",
    "@types/express": "^4.16.1",
    "@types/fs-extra": "^5.0.4",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/mocha": "^5.2.5",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.7",
    "@types/node": "^10.12.18",
    "@types/supertest": "^2.0.7",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "nodemon": "^1.18.9",
    "supertest": "^3.4.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typedoc": "^0.13.0",
    "typescript": "~3.1.6"
  }
}
